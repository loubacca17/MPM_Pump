    </Method>
    <Method Name="M_GetJsonString" Id="{6c0a32eb-aa79-412c-a85a-e47532b90cb3}">
      <Declaration><![CDATA[METHOD PUBLIC M_GetJsonString : HResult
VAR_IN_OUT CONSTANT
	pRecipeDoc : STRING(10000);
	pSystemDoc : STRING(10000);
	pIODoc : STRING(10000);
	pAxisDoc : STRING(10000);	
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[fbRecipeJson.CopyDocument(pRecipeDoc,SIZEOF(pRecipeDoc));
fbSystemJson.CopyDocument(pSystemDoc,SIZEOF(pSystemDoc));
fbIOJson.CopyDocument(pIODoc,SIZEOF(pIODoc));]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_SetJsonIO" Id="{132bf0dc-f494-4655-8dce-d202cd22afc3}">
      <Declaration><![CDATA[METHOD PUBLIC M_SetJsonIO : HResult
VAR_INPUT
END_VAR
VAR CONSTANT
	uSizeOfStAnyType : UDINT := SIZEOF(PVOID);
END_VAR
VAR_INST
	uSizeOfArray : DINT;
	x : DINT;
	pFBConfigJson : POINTER TO POINTER TO FB_ConfigJson;	
	lastResult : HRESULT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[
uSizeOfArray := ua_pFBConfigJson/uSizeOfStAnyType - 1; //get number of items in array


fbIOJson.M_CreateSchemaJson();
FOR x := 0 TO uSizeOfArray BY 1 DO
	pFBConfigJson := a_pFBConfigJson + uSizeOfStAnyType*x;
	lastResult := pFBConfigJson^^.F_IO(lastResult,fbIOJson.P_sJsonValues.jsonValue,ADR(fbIOJson),FALSE);
END_FOR]]></ST>
      </Implementation>
    </Method>
